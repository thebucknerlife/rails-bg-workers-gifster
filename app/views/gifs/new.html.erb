<h2></h2>

<%= form_for @gif do |f| %>
  <h4>Upload file or URL (one or the other)</h4>
  <%= f.text_field :remote_image_url, placeholder: 'paste image url here' %>
  <%= f.file_field :image %>
  <h4>Title</h4>
  <%= f.text_field :title, placeholder: 'title goes here' %>
  <%= f.submit 'POST' %>
<% end %>